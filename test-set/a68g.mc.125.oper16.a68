               tstb (212, lt0[-i], j > 0);
               tstb (213, NOT lt0[i], j >= 0);
               tstb (214, lt0[i], j < 0);
               tstb (215, NOT lt0[-i], j <= 0)
            OD
      END;
      print (("Test: monadic -", newline));
