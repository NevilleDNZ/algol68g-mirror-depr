            tstb (163, TRUE, a8);
            tstb (164, TRUE, a9);
            tstb (165, FALSE, a10);
            tstb (166, FALSE, b1);
            tstb (167, FALSE, b2);
            tstb (168, TRUE, b3)
      END;
      print (("Test: EQ, NE, LT, LE, GT, GE", newline));
      BEGIN IF -1 = -1
            THEN SKIP
            ELSE tste (169)
            FI;
            tstb (170, TRUE, -LONG 1 = -LONG 1);
            IF -1 /= 1
            THEN SKIP
            ELSE tste (171)
            FI;
            tstb (172, TRUE, -LONG 1 /= LONG 1);
            IF 1 /= -1
